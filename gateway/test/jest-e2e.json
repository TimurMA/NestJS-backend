{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/library/(.*)": "<rootDir>/../libs/library/src/$1",
    "@app/library": "<rootDir>/../libs/library/src",
    "@entity/entities/(.*)": "<rootDir>/../libs/entities/src/$1",
    "@entity/entities": "<rootDir>/../libs/entities/src"
  },
  "globalSetup": "../src/jest.setup.ts",
  "globalTeardown": "../src/jest.teardown.ts"
}